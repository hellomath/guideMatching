<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:sl="http://schemas.android.com/apk/res/com.kimtajo.guideMatching"
                android:id="@+id/main_relativelayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/background2"
                >

    <widget.ScalableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            sl:scale_base_width="720"
            sl:scale_base_height="1280"
            android:layout_centerVertical="true"
            android:id="@+id/view">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gpsLocationOfSchedule"
                sl:scale_top="30"
                sl:scale_left="30"
                sl:scale_height="135"
                sl:scale_width="675"
                sl:scale_textsize="50"
                android:textColor="#343030"
                android:background="#FFFFFF"
                android:text="Loading Location.."
                android:gravity="center"/>
    <ListView
            android:id="@+id/android:list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:cacheColorHint="#00000000"
            sl:scale_left="30"
            sl:scale_top="205"
            sl:scale_height="760"
            sl:scale_width="675"
            android:textColor="#ffffff"
            android:layout_gravity="center_horizontal"/>

        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/locationOfSchedule"
                sl:scale_left="30"
                sl:scale_top="1105"
                sl:scale_height="140"
                sl:scale_width="140"
                sl:scale_textsize="30"
                android:scaleType="fitXY"
                android:background="@android:color/transparent"
                android:src="@drawable/find_area_menu"/>

        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/friendOfSchedule"
                sl:scale_left="200"
                sl:scale_top="1105"
                sl:scale_height="140"
                sl:scale_width="140"
                sl:scale_textsize="30"
                android:scaleType="fitXY"
                android:background="@android:color/transparent"
                android:src="@drawable/friend_menu"/>

        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scheduleOfSchedule"
                sl:scale_left="390"
                sl:scale_top="1105"
                sl:scale_height="140"
                sl:scale_width="140"
                sl:scale_textsize="30"
                android:scaleType="fitXY"
                android:background="@android:color/transparent"
                android:src="@drawable/schedule_menu"/>

        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/optionOfSchedule"
                sl:scale_left="560"
                sl:scale_top="1105"
                sl:scale_height="140"
                sl:scale_width="140"
                sl:scale_textsize="30"
                android:scaleType="fitXY"
                android:background="@android:color/transparent"
                android:src="@drawable/option_menu"/>
    </widget.ScalableLayout>
</RelativeLayout>