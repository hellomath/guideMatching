<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:sl="http://schemas.android.com/apk/res/com.kimtajo.guideMatching"
                android:id="@+id/main_relativelayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#353030"
        >

    <widget.ScalableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            sl:scale_base_width="720"
            sl:scale_base_height="1280"
            android:layout_centerVertical="true"
            android:id="@+id/view">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                sl:scale_top="30"
                sl:scale_left="30"
                sl:scale_height="135"
                sl:scale_width="675"
                sl:scale_textsize="50"
                android:textColor="#343030"
                android:background="#FFFFFF"
                android:text="Detail Schedule"
                android:gravity="center"/>


        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scdNameDetail"
                sl:scale_top="205"
                sl:scale_left="30"
                sl:scale_height="70"
                sl:scale_width="675"
                sl:scale_textsize="50"
                android:textColor="#FFFFFF"
                android:text="City Tour"
                android:gravity="left"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scdDatetimeDetail"
                sl:scale_top="265"
                sl:scale_left="30"
                sl:scale_height="50"
                sl:scale_width="675"
                sl:scale_textsize="40"
                android:textColor="#FFFFFF"
                android:text="Seoul City hall"
                android:gravity="left"/>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                sl:scale_top="310"
                sl:scale_left="30"
                sl:scale_height="40"
                sl:scale_width="675"
                sl:scale_textsize="30"
                android:id="@+id/scdInfoDetail"
                android:text="Seoul City Hall visit asdasdas"
                />
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                sl:scale_top="345"
                sl:scale_left="30"
                sl:scale_height="40"
                sl:scale_width="675"
                sl:scale_textsize="30"
                android:id="@+id/scdEntryDetail"
                android:text="5 people"
                />
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scdMemoDetail"
                sl:scale_top="450"
                sl:scale_left="30"
                sl:scale_height="135"
                sl:scale_width="675"
                sl:scale_textsize="30"
                android:textColor="#343030"
                android:background="#FFFFFF"
                android:text="scdMemo"/>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scdMeetListDetail"
                sl:scale_top="635"
                sl:scale_left="30"
                sl:scale_height="70"
                sl:scale_width="675"
                sl:scale_textsize="50"
                android:textColor="#343030"
                android:background="#FFFFFF"
                android:text="Meet List"
                android:gravity="center"/>

        <ListView
                android:id="@+id/android:list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:cacheColorHint="#00000000"
                sl:scale_top="710"
                sl:scale_left="30"
                sl:scale_height="550"
                sl:scale_width="675"
                android:textColor="#ffffff"
                android:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scdJoinDetail"
                sl:scale_top="205"
                sl:scale_left="530"
                sl:scale_height="180"
                sl:scale_width="180"
                android:scaleType="fitXY"
                android:background="@android:color/transparent"
                android:src="@drawable/friend_add"/>
    </widget.ScalableLayout>



</RelativeLayout>